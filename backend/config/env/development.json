{
  "loadSwagger": true,
  "typeorm": {
    "type": "sqlite",
    "database": "./db.db",
    "synchronize": true,
    "entities": [
      "src/entities/**/*.ts"
    ],
    "logging": true,
    "migrations": [
      "migrations/*.ts"
    ],
    "cli": {
      "migrationsDir": "migrations"
    }
  },
  "logger": {
    "level": "info",
    "prettyPrint": true
  }
}
